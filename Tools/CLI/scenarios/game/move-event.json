{
  "maxDuration": 15000,
  "steps": [
    {
      "type": "log",
      "message": "Testing MoveToEvent with complex payload (Position2)"
    },
    {
      "type": "event",
      "event": "MoveToEvent",
      "payload": {
        "target": {
          "v": {
            "x": 50000,
            "y": 30000
          }
        }
      }
    },
    {
      "type": "wait",
      "wait": 1000
    },
    {
      "type": "assert",
      "assert": {
        "path": "players",
        "exists": true,
        "message": "players should exist in state"
      }
    },
    {
      "type": "log",
      "message": "MoveToEvent sent successfully - player should be moving towards target position"
    },
    {
      "type": "event",
      "event": "UpgradeWeaponEvent",
      "payload": {}
    },
    {
      "type": "wait",
      "wait": 500
    },
    {
      "type": "log",
      "message": "UpgradeWeaponEvent sent (may fail if not enough resources)"
    }
  ]
}
