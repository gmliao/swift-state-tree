{
  "name": "swiftstatetree-cli",
  "version": "1.0.0",
  "description": "SwiftStateTree WebSocket CLI testing tool",
  "type": "module",
  "bin": {
    "swiftstatetree-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "measure": "tsx benchmarks/measure-state-updates.ts",
    "measure:full": "bash benchmarks/run-protocol-measurement.sh",
    "test:protocol": "npm run test-mixed-encoding && npm run test-packet-encoding && npm run test-encoding-integration && npm run test-server-opcode-decoding",
    "test:e2e": "tsx src/cli.ts script -u ws://localhost:8080/game -l demo-game -s scenarios/",
    "test:e2e:all": "npm run test:e2e -- --state-update-encoding jsonObject && npm run test:e2e -- --state-update-encoding opcodeJsonArray",
    "test": "npm run test:protocol && npm run test:e2e:all",
    "test-mixed-encoding": "tsx scripts/internal/test-mixed-encoding.ts",
    "test-mixed-encoding-server": "tsx scripts/internal/test-mixed-encoding-server.ts",
    "test-packet-encoding": "tsx scripts/internal/test-packet-encoding.ts",
    "test-encoding-integration": "tsx scripts/internal/test-encoding-integration.ts",
    "test-server-opcode-decoding": "tsx scripts/internal/test-server-opcode-decoding.ts"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.3",
    "@swiftstatetree/sdk": "file:../../sdk/ts",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
