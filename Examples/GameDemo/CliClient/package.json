{
  "name": "hero-defense-cli-client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "npm run build --prefix ../../../sdk/ts && npm run schema:generate && npm run codegen",
    "dev": "tsx src/cli.ts",
    "test": "tsx src/auto-test.ts",
    "test:verbose": "tsx src/auto-test.ts --mode=verbose",
    "test:normal": "tsx src/auto-test.ts --mode=normal",
    "prebuild": "npm run build --prefix ../../../sdk/ts && npm run schema:generate && npm run codegen",
    "build": "tsc",
    "start": "node dist/cli.js",
    "schema:generate": "cd .. && swift run SchemaGen --output schema.json",
    "precodegen": "npm run build --prefix ../../../sdk/ts",
    "codegen": "rm -rf ./src/generated && node ../../../sdk/ts/dist/cli.js codegen --input ../schema.json --output ./src/generated --framework vue",
    "generate": "npm run schema:generate && npm run codegen"
  },
  "dependencies": {
    "@swiftstatetree/sdk": "file:../../../sdk/ts",
    "chalk": "^5.3.0",
    "readline": "^1.3.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3"
  }
}
