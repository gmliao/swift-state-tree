{
  "name": "@swiftstatetree/sdk",
  "version": "0.1.0",
  "description": "SwiftStateTree TypeScript SDK - Runtime and View architecture",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "bin": {
    "@swiftstatetree/sdk": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "codegen": "node ./dist/cli.js codegen --input ./schema.json --output ./src/generated"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "ws": "^8.16.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  }
}

